            <div class="PostalCode">
                <label id="label_PostalCode">Zip</label>
                <div><input type="text" id="PostalCode" class="" /><!-- <span class="okNet"></span> --></div>
            </div>
            <input type="hidden" id="CityTown" value="" />
            <input type="hidden" id="State" value="" />
            <script type="text/javascript">
                $(document).ready(function() {
                    $("#PostalCode").blur(function() {
                        var zipCodeInput = $('#PostalCode');
                        if (zipCodeInput.length > 0) {
                            var countryId = $('#regCountrySelect option:selected').attr('data-country');
                            HandleZipCodeValidation(parseInt(countryId), false);
                        }
                    });
                    $("#PostalCode").autocomplete(
                        {
                            source: function(request, response) {
                                $.ajax({
                                    url: '/MiscAjax/GetAutocompleteZip',
                                    dataType: "json",
                                    data: {
                                        term: request.term,
                                        country: 'us'
                                    },
                                    success: function(data) {
                                        if (!data) {
                                            response('');
                                        }
                                        response($.map(data, function(item) {
                                            var itemArr = item.split('|');
                                            return {
                                                label: itemArr[0] + ', ' + itemArr[1],
                                                value: itemArr[0],
                                                city: itemArr[1],
                                                state: itemArr[2]
                                            };
                                        }));
                                    }
                                });
                            },
                            open: function(event, ui) {
                                var objAutocomplete = $(".ui-autocomplete").eq(0);
                                objAutocomplete.css("width", 257);                              
                            },
                            select: function(event, ui) {
                                if (ui.item) {                                    
                                    $('#CityTown').val(ui.item.city);
                                    $('#State').val(ui.item.state);
                                    //selectStateAutocomplete(ui.item.state);
                                    //HandleCityValidation();
                                    
                                    setTimeout(function() {
                                        var zipCodeInput = $('#PostalCode');
                                        if (zipCodeInput.length > 0) {
                                            var countryId = $('#regCountrySelect option:selected').attr('data-country');
                                            HandleZipCodeValidation(parseInt(countryId), false);
                                        }    
                                    },100);
                                    
                                }
                            },
                            minLength: 2
                        });
                });
            </script>

        
