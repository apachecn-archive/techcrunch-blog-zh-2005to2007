  }
  bcrumbs.innerHTML = existingText;

  if(includeBottom) {
      addBCrumbsBottom(link, msg);
  }
}
//empty the
function resetBCrumbs() {
  document.getElementById('bcrumbs').innerHTML = "";
}

var bcrumbsBottomContent = null; 
function addBCrumbsBottom(link, msg) {
  if(null == msg || "" == msg || null == link || "" == link){
    return;
  }
  if(bcrumbsBottomContent==null || bcrumbsBottomContent=="" || trim(bcrumbsBottomContent)==""){
      bcrumbsBottomContent = '<b>Back to:</b> ';
  } else {
      bcrumbsBottomContent += " | ";
  }
  bcrumbsBottomContent += '<a href="'+link+'"><strong>'+msg+'</strong></a>';
}

function trim(s){
  return s.replace(/^\s*|\s*$/g,"");
}
