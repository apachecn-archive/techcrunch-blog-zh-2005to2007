      if (!ob || String(ob.className).match(/\bsubnav\b/)) {
        break;
      }
      if (String(ob.tagName).toUpperCase() == 'LI') {
        ob.className += ' active';
      }
      ob = ob.parentElement;
    }
  }

  var subNav = document.getElementById('tthdrsubnav');
  if (subNav) {
    var links = subNav.getElementsByTagName('a');
    if (links.length) {
      var curLoc = '' + document.location;
      var numLinks = links.length;
      var matchedElem = null;
      var matchedDepth = 0;
      var i;
      for (i = 0; i < numLinks; i++) {
        var item = links[i];
        if (window.mapHost) {
          var href = String(item.getAttribute('href'));
          var newHref = href.replace(mapHost, document.location.host);
          if (href != newHref) {
              item.setAttribute('href', newHref);
          }
        }

        if (matchLink(item, curLoc)) {
          var depth = getLinkDepth(item);
          if (depth > matchedDepth) {
            matchedDepth = depth;
            matchedElem = item;
          }
        }
      }

      if (matchedElem) {
        // Highlight element.
        highlightLink(matchedElem);
      }
    }
  }
})();
