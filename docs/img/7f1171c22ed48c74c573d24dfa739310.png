(function(){
  function matchUri(uri, currentUri, exactMatch) {
    uri = String(uri).replace(/^https:/,'http:');
    currentUri = String(currentUri).replace(/^https:/,'http:');
    var res = false;
    if (uri == currentUri) {
        res = true;
    } else if (!exactMatch) {
        // Test for prefix
        var urls = [];
        if (uri.indexOf('?') == -1) {
          urls[urls.length] = uri + '?';
          var len = uri.length;
          if (uri.lastIndexOf('/') == (len-1)) {
             urls[urls.length] = uri;
          } else {
             urls[urls.length] = uri + '/';
          }
        }
        var i;
        for (i = 0;i < urls.length; i++) {
          if (currentUri.indexOf(urls[i]) === 0) {
             // Prefix match.
             res = true;
             break;
          }
        }
    }
    return res;
  }

  function matchLink(item, currentUri) {
